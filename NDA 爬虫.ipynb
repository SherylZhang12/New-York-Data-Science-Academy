    "import pandas as pd\n",
    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "import time\n",
    "from selenium.webdriver.chrome.service import Service\n",
    "\n",
    "# 80家公司列表\n",
    "companies = [\n",
    "    # 金融（前27）\n",
    "    \"JPMorgan Chase & Co.\", \"Goldman Sachs\", \"Morgan Stanley\", \"Bank of America\", \"Citigroup\",\n",
    "    \"BlackRock\", \"Fidelity Investments\", \"Prudential Financial\", \"The Hartford\", \"Bloomberg L.P.\",\n",
    "    \"American Express\", \"Mastercard\", \"Moody's Corporation\", \"S&P Global\", \"BNY Mellon\",\n",
    "    \"State Street Corporation\", \"Travelers\", \"Chubb\", \"Marsh McLennan\", \"AIG\",\n",
    "    \"Capital One\", \"TD Bank Group\", \"UBS\", \"Barclays\", \"Deutsche Bank\", \"Macquarie Group\", \"AllianceBernstein\",\n",
    "\n",
    "    # 医疗（中27）\n",
    "    \"Johnson & Johnson\", \"Pfizer\", \"Merck\", \"Bristol Myers Squibb\", \"CVS Health\",\n",
    "    \"Cigna\", \"Boston Scientific\", \"Stryker Corporation\", \"Becton, Dickinson and Company (BD)\",\n",
    "    \"Regeneron Pharmaceuticals\", \"Vertex Pharmaceuticals\", \"Moderna\", \"IQVIA\", \"Quest Diagnostics\",\n",
    "    \"Hologic\", \"Mount Sinai Health System\", \"NewYork-Presbyterian Hospital\", \"Northwell Health\",\n",
    "    \"Mass General Brigham\", \"Dana-Farber Cancer Institute\", \"UnitedHealth Group\", \"Anthem (Elevance Health)\",\n",
    "    \"Zoetis\", \"Biogen\", \"Gilead Sciences\", \"Eli Lilly and Company\", \"Teva Pharmaceutical Industries\",\n",
    "\n",
    "    # IT/科技（后26）\n",
    "    \"IBM\", \"Verizon\", \"Oracle\", \"Dell Technologies\", \"SAP\", \"Cisco Systems\", \"Adobe\", \"Salesforce\",\n",
    "    \"Microsoft\", \"Amazon\", \"Google\", \"Meta Platforms\", \"Comcast\", \"Cognizant\", \"Automatic Data Processing (ADP)\",\n",
    "    \"NVIDIA\", \"Corning Incorporated\", \"Thomson Reuters\", \"Dassault Systèmes\", \"PTC\",\n",
    "    \"Akamai Technologies\", \"Red Hat\", \"Wayfair\", \"Tripadvisor\", \"Gartner\", \"ServiceNow\"\n",
    "]\n",
    "\n",
    "# 搜索关键词（统一设为数据科学相关负责人）\n",
    "keywords = [\"Head of Data Science / CTO / VP of Innovation\"] * len(companies)\n",
    "\n",
    "results = []\n",
    "\n",
    "# Chrome 配置\n",
    "options = webdriver.ChromeOptions()\n",
    "# options.add_argument(\"--headless\")  # 运行时可取消注释以显示浏览器\n",
    "options.add_argument(\"--disable-gpu\")\n",
    "options.add_argument(\"--disable-blink-features=AutomationControlled\")\n",
    "options.add_argument(\"--log-level=3\")\n",
    "\n",
    "# ChromeDriver的路径\n",
    "service = Service(\"/Users/mac/Downloads/chromedriver-mac-x64/chromedriver\")\n",
    "driver = webdriver.Chrome(service=service, options=options)\n",
    "\n",
    "\n",
    "# 开始搜索\n",
    "for company, keyword in zip(companies, keywords):\n",
    "    query = f\"{keyword} {company} site:linkedin.com/in\"\n",
    "    url = f\"https://www.bing.com/search?q={query.replace(' ', '+')}\"\n",
    "\n",
    "    print(f\"🔍 正在搜索：{company}\")\n",
    "    driver.get(url)\n",
    "    time.sleep(2)\n",
    "\n",
    "    found_link = \"Not Found\"\n",
    "\n",
    "    try:\n",
    "        links = driver.find_elements(By.CSS_SELECTOR, 'li.b_algo h2 a')\n",
    "        for link in links:\n",
    "            href = link.get_attribute(\"href\")\n",
    "            if href and \"linkedin.com/in\" in href:\n",
    "                found_link = href\n",
    "                break\n",
    "    except Exception as e:\n",
    "        print(f\"错误：{e}\")\n",
    "        found_link = \" Error\"\n",
    "\n",
    "    results.append({\n",
    "        \"Company Name\": company,\n",
    "        \"Search Keyword\": keyword,\n",
    "        \"LinkedIn Profile\": found_link\n",
    "    })\n",
    "\n",
    "    time.sleep(1.5)  # 防止访问太快被封\n",
    "\n",
    "driver.quit()\n",
    "\n",
    "# 输出结果到Excel\n",
    "df_result = pd.DataFrame(results)\n",
    "df_result.to_excel(\"NDA_80_Company_Contact_Results_Bing.xlsx\", index=False)\n",
    "\n",
    "print(\"全部完成！联系人链接已保存到 NDA_80_Company_Contact_Results_Bing.xlsx\")\n"
   ]
